{
  "name": "tamber-react-demo",
  "version": "0.0.1",
  "description": "Tamber react app demo.",
  "repository": "https://github.com/tamber/tamber-react-demo",
  "main": "js/app.js",
  "dependencies": {
    "tamber": "^1.0.6",
    "flux": "^2.1.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-svg-icons": "^0.2.0",
    "jquery": "^3.1.1",
    "react-notification": "^5.0.0",
    "immutable": "^3.7.4",
    "react-syntax-highlighter": "^5.0.0",
    "json-loader": "^0.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "envify": "^3.4.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.6.2",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] -t [babelify --presets [es2015 react]] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "author": "Alexi Robbins",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ],
      "envify"
    ]
  },
  "react-svg-icons": {
    "assetPath": "./img/svg"
  },
  "jest": {
    "rootDir": "./js",
    "scriptPreprocessor": "../node_modules/babel-jest"
  }
}
